<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css"
        integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Josefin+Sans&family=Open+Sans:ital,wght@1,300&family=Roboto+Condensed:ital@1&display=swap');
    </style>
    <style type='text/css'>
        body {
            color: black;
            display: table;
            font-family: 'Oswald', sans-serif;
            font-size: 14px;
            text-align: center;
        }

        p {
            font-family: 'Open Sans', sans-serif;
            padding-right: 10px;
        }


        .table-suply {
            border: none;
            border-top: none;
            margin-top: 20px;
        }

        .table-suply tr th,
        .table-suply tr td {
            border-top: none;
            text-align: left;
            border-right: none;
            padding: 1px;

        }

        .table-suply tr th:last-child,
        .table-suply tr td:last-child {
            border-right: none;
        }









        #backLogo {
            background: url(tree.png);
            background-repeat: no-repeat;
            background-size: auto;
            margin-top: 25%;
            background-size: 400px 400px;
        }

        footer {
            position: fixed;
            bottom: -20px;
            left: 0px;
            right: 0px;
            height: 70px;
            font-size: 12px !important;

            /** Extra personal styles **/
            background-color: white;
            color: black;
            text-align: center;
        }
    </style>
</head>

<body>
    <center style="margin: 0px auto;">
        <footer>
            <span>
                King Fahad Branch Road, Al-Moatamarat District
                P.O.Box 16683 Riyadh 11474, Saudi Arabia
            </span>
        </footer>

        <hr style="width: 100%; border: 10px solid #EF6231;">
        </hr>

        <table class="table-suply" width="100%" cellspacing="0" cellpadding="0" style="border:none; line-height: 0.5rem;">
            <tr>
                <td width="70%">
                    <table>
                        <tr style="width: 60%" class="text-left">
                            <td width="50%">
                                <img src="<%= BACKEND_URL + '/backend/images/logo/' + general.logo %>" width="210" height="100">
                            </td>
                            <td width="50%">
                                <p style="margin-bottom:0; text-align: left;">The Global Language of Business</p>
                                <p style="margin : 0; padding-top:0; text-align: left; font-size: bold; font-size: 16px; color: #F26334;">
                                    <strong>GS<span>1</span> COMPANY PREFIX CERTIFICATE</strong>
                                </p>
                            </td>
                        </tr>

                        <tr>
                            <td>
                                <p style="margin-bottom:0;">Licensed To</p>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <p style="margin-bottom:0; padding-top:0; padding-bottom: 0; font-size: 20px; color: #F26334;">
                                    <span><%= memberData.company_name_eng %></span>
                                </p>
                            </td>
                        </tr>

                        <!-- ... more rows ... -->

                        <tr>
                            <td>
                                <p style="margin-bottom:0; padding-top:0;"><strong>GS<span>1</span> GLN Identification:
                                        <span style="color: #F26334;"><%= memberData.gln %></span></strong></p>
                            </td>
                            <td class="text-center">
                                <img src="<%= BACKEND_URL + '/upload/stamp1.png' %>" width="120" height="100" style="transform: rotate(330deg);">
                            </td>
                        </tr>

                        <!-- ... more rows ... -->

                        <tr>
                            <td class="text-left">
                                <span>MemberID: </span>&nbsp;&nbsp;<span><%= memberData.companyID %></span>
                            </td>
                            <td class="text-left">
                                <span>Valid Until: </span>&nbsp;&nbsp;<span><%= currentDate.day %>-<%= currentDate.month %>-<%= currentDate.year %></span>
                            </td>
                        </tr>
                    </table>
                </td>

                <td width="30%" style="float: left; margin-top: 30%;">
                    <table>
                        <!-- ... table rows ... -->
                        <tr>
                            <td>
                                <img src="<%= BACKEND_URL + '/upload/tree.png' %>" width="400" height="400">
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>
    </center>
</body>


</html>